{
  "defaultStacks": [
    "frontend"
  ],
  "packages": {
    "s3": {
      "type": "aws-s3",
      "data": {
        "bucket": "aws-frontend-artifacts",
        "cacheControl": "public, max-age=315360000",
        "prefixPackage": true,
        "prefixStack": false,
        "prefixStage": true,
        "publicReadAcl": false
      }
    },
    "lambda": {
      "type": "aws-lambda",
      "data": {
        "functions": {
          "PROD": {
            "name": "frontend-facia-purger-PROD-Lambda-1WN5JINVN30WY",
            "filename": "facia-purger.jar"
          }
        }
      }
    }
  },
  "recipes": {
    "upload": {
      "actionsBeforeApp": [
        "s3.uploadStaticFiles"
      ]
    },
    "lambda": {
      "actionsBeforeApp": [
        "lambda.updateLambda"
      ]
    },
    "default": {
      "depends": [
        "upload",
        "lambda"
      ]
    }
  }
}